{
  "name": "scss-rules",
  "version": "1.0.0",
  "description": "A collection of SCSS rules missing in stylelint-scss",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "npm run lint-js",
    "lint-js": "bash ./config/bin/lint-js",
    "jest": "jest",
    "watch": "jest --watch",
    "test-rule": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "3.19.0",
    "eslint-config-funbox": "2.1.0",
    "jest": "^23.1.0",
    "postcss": "^6.0.22",
    "postcss-scss": "^1.0.5"
  },
  "jest": {
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testRegex": "src/.*/__tests__/.*\\.js$"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "stylelint": "^9.2.1",
    "stylelint-scss": "^3.1.0"
  }
}
